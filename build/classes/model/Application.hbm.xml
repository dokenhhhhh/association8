<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Application" table="application" schema="club">
        <id name="applicationId" column="ApplicationId"/>
        <property name="applicationNum" column="ApplicationNum"/>
        <property name="applicationContent" column="ApplicationContent"/>
        <property name="applicationTime" column="ApplicationTime"/>
        <property name="applicationState" column="ApplicationState"/>
        <property name="applicationResult" column="ApplicationResult"/>
        <property name="applicationType" column="ApplicationType"/>
    </class>
</hibernate-mapping>