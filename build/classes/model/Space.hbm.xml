<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Space" table="space" schema="club">
        <id name="spaceId" column="SpaceId"/>
        <property name="spaceName" column="SpaceName"/>
        <property name="spaceState" column="SpaceState"/>
    </class>
</hibernate-mapping>